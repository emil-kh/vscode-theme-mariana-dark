{
    "name": "Sublime Text 4 Theme",
    "type": "dark",
    "colors": {
        "activityBar.background": "#2F333D",
        "activityBar.foreground": "#D7DAE0",
        "activityBarBadge.background": "#528bff",
        "activityBarBadge.foreground": "#F8FAFD",
        "button.background": "#528bff",
        "debugToolBar.background": "#2F333D",
        "diffEditor.insertedTextBackground": "#00809B33",
        "dropdown.background": "#1d1f23",
        "dropdown.border": "#181A1F",
        "editor.background": "#282c34",
        "editor.findMatchBackground": "#42557B",
        "editor.findMatchHighlightBackground": "#314365",
        "editor.lineHighlightBackground": "#383E4A",
        "editor.selectionBackground": "#3E4451",
        "editorCursor.foreground": "#f8f8f0",
        "editorError.foreground": "#c24038",
        "editorGroup.emptyBackground": "#181A1F",
        "editorGroup.border": "#181A1F",
        "editorGroupHeader.tabsBackground": "#21252B",
        "editorIndentGuide.background": "#3B4048",
        "editorHoverWidget.background": "#21252B",
        "editorHoverWidget.border": "#181A1F",
        "editorLineNumber.foreground": "#495162",
        "editorRuler.foreground": "#484848",
        "editorSuggestWidget.background": "#21252B",
        "editorSuggestWidget.border": "#181A1F",
        "editorSuggestWidget.selectedBackground": "#2c313a",
        "editorUnnecessaryCode.opacity": "#000000c0",
        "editorWhitespace.foreground": "#484a50",
        "editorWidget.background": "#21252B",
        "input.background": "#1d1f23",
        "list.activeSelectionBackground": "#2c313a",
        "list.activeSelectionForeground": "#d7dae0",
        "list.focusBackground": "#383E4A",
        "list.highlightForeground": "#C5C5C5",
        "list.hoverBackground": "#292d35",
        "list.inactiveSelectionBackground": "#2c313a",
        "list.inactiveSelectionForeground": "#d7dae0",
        "notifications.background": "#21252b",
        "scrollbarSlider.activeBackground": "#747D9180",
        "scrollbarSlider.background": "#4E566680",
        "scrollbarSlider.hoverBackground": "#5A637580",
        "sideBar.background": "#21252b",
        "sideBarSectionHeader.background": "#282c34",
        "statusBar.background": "#21252B",
        "statusBar.foreground": "#9da5b4",
        "statusBarItem.hoverBackground": "#2c313a",
        "statusBar.noFolderBackground": "#21252B",
        "statusBar.debuggingBackground": "#21252B",
        "tab.activeBackground": "#383E4A",
        "tab.border": "#181A1F",
        "tab.inactiveBackground": "#21252B",
        "terminal.foreground": "#abb2bf",
        "terminal.ansiBlack": "#2d3139",
        "terminal.ansiBlue": "#528bff",
        "terminal.ansiGreen": "#98c379",
        "terminal.ansiYellow": "#e5c07b",
        "terminal.ansiCyan": "#56b6c2",
        "terminal.ansiMagenta": "#c678dd",
        "terminal.ansiRed": "#e06c75",
        "terminal.ansiWhite": "#d7dae0",
        "terminal.ansiBrightBlack": "#7f848e",
        "terminal.ansiBrightBlue": "#528bff",
        "terminal.ansiBrightGreen": "#98c379",
        "terminal.ansiBrightYellow": "#e5c07b",
        "terminal.ansiBrightCyan": "#56b6c2",
        "terminal.ansiBrightMagenta": "#7e0097",
        "terminal.ansiBrightRed": "#f44747",
        "terminal.ansiBrightWhite": "#d7dae0",
        "titleBar.activeBackground": "#282c34",
        "titleBar.activeForeground": "#9da5b4",
        "titleBar.inactiveBackground": "#282C34",
        "titleBar.inactiveForeground": "#6B717D"
      },
    "tokenColors": [
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#A6ACB9"
        }
      },
      {
        "scope": ["string.quoted.single", "string.quoted.double"],
        "settings": {
          "foreground": "#99C794"
        }
      },
      {
        "scope": ["punctuation.definition"],
        "settings": {
          "foreground": "#5FB4B4"
        }
      },
      {
        "scope": "constant.numeric",
        "settings": {
          "foreground": "#F9AE58"
        }
      },
      {
        "scope": "constant.language",
        "settings": {
          "foreground": "#f08288",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["constant.character", "constant.other"],
        "settings": {
          "foreground": "#C695C6"
        }
      },
      {
        "scope": ["variable.member", "variable.other.constant.property"],
        "settings": {
          "foreground": "#f08288"
        }
      },
      {
        "scope": [
          "keyword.operator.word",
          "keyword.control",
          "keyword.operator.new"
        ],
        "settings": {
          "foreground": "#C695C6"
        }
      },
      {
        "scope": [
          "string.template",
          "meta.template.expression",
          "meta.embedded.line"
        ],
        "settings": {
          "foreground": "#99C794"
        }
      },
      {
        "scope": ["variable.other.object.property"],
        "settings": {
          "foreground": "#d8dee9"
        }
      },
      {
        "scope": "keyword.operator",
        "settings": {
          "foreground": "#fa977b"
        }
      },
      {
        "scope": ["punctuation.separator", "punctuation.terminator"],
        "settings": {
          "foreground": "#A6ACB9"
        }
      },
      {
        "scope": "punctuation.section",
        "settings": {
          "foreground": "#d8dee9"
        }
      },
      {
        "scope": "punctuation.accessor",
        "settings": {
          "foreground": "#A6ACB9"
        }
      },
      {
        "scope": "punctuation.definition.annotation",
        "settings": {
          "foreground": "#5FB4B4"
        }
      },
      {
        "scope": [
          "variable.other.dollar.only",
          "variable.other.object.dollar.only",
          "variable.type.dollar.only",
          "support.class.dollar.only"
        ],
        "settings": {
          "foreground": "#5FB4B4"
        }
      },
      {
        "scope": ["storage.data.table.abl"],
        "settings": {
          "foreground": "#698ab5"
        }
      },
      {
        "scope": ["meta.define.class.abl"],
        "settings": {
          "foreground": "#698ab5"
        }
      },
      {
        "scope": ["variable.other.abl"],
        "settings": {
          "foreground": "#d7dade"
        }
      },
      {
        "scope": ["string.double.complex.abl"],
        "settings": {
          "foreground": "#a1aebf"
        }
      },
      {
        "scope": ["source.abl"],
        "settings": {
          "foreground": "#282C34"
        }
      },
      {
        "scope": ["keyword.other.abl"],
        "settings": {
          "foreground": "#ae95c6"
        }
      },
      {
        "scope": ["storage.type.abl"],
        "settings": {
          "foreground": "#f08288"
        }
      },
      {
        "scope": "entity.name.function.abl",
        "settings": {
          "foreground": "#86b9eb"
        }
      },
      {
        "scope": "meta.object-literal.key",
        "settings": {
          "foreground": "#6699CC"
        }
      },
      {
        "scope": "entity.other.inherited-class",
        "settings": {
          "foreground": "#5FB4B4",
          "fontStyle": "italic underline"
        }
      },
      {
        "scope": "variable.parameter",
        "settings": {
          "foreground": "#F9C669",
          "fontStyle": ""
        }
      },
      {
        "scope": "variable.language",
        "settings": {
          "foreground": "#f08288",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "entity.name.tag",
        "settings": {
          "foreground": "#f08288",
          "fontStyle": ""
        }
      },
      {
        "scope": "entity.other.attribute-name",
        "settings": {
          "foreground": "#C594C5"
        }
      },
      {
        "scope": ["variable.function", "variable.annotation"],
        "settings": {
          "foreground": "#6699CC",
          "fontStyle": ""
        }
      },
      {
        "scope": ["support.function", "support.macro"],
        "settings": {
          "foreground": "#6699CC",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "support.constant",
        "settings": {
          "foreground": "#C594C5",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["support.type", "support.class"],
        "settings": {
          "foreground": "#86b9eb"
        }
      },
      {
        "scope": ["support.type.object.module"],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "invalid",
        "settings": {
          "foreground": "#FFFFFF70"
        }
      },
      {
        "scope": "invalid.deprecated",
        "settings": {
          "foreground": "#d8dee9"
        }
      },
      {
        "scope": "entity.name.tag.yaml",
        "settings": {
          "foreground": "#5FB4B4"
        }
      },
      {
        "scope": "source.yaml string.unquoted",
        "settings": {
          "foreground": "#d8dee9"
        }
      },
      {
        "scope": "markup.heading",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": "markup.heading punctuation.definition.heading",
        "settings": {
          "foreground": "#f08288"
        }
      },
      {
        "scope": "markup.heading.1 punctuation.definition.heading",
        "settings": {
          "foreground": "#f08288"
        }
      },
      {
        "scope": ["string.other.link", "markup.underline.link"],
        "settings": {
          "foreground": "#6699CC"
        }
      },
      {
        "scope": "markup.bold",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": "markup.italic",
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": "markup.italic markup.bold | markup.bold markup.italic",
        "settings": {
          "fontStyle": "bold italic"
        }
      },
      {
        "scope": "punctuation.definition.thematic-break",
        "settings": {
          "foreground": "#FAB763"
        }
      },
      {
        "scope": "markup.list.numbered.bullet",
        "settings": {
          "foreground": "#A3CE9E"
        }
      },
      {
        "scope": [
          "markup.quote punctuation.definition.blockquote",
          "markup.list punctuation.definition.list_item"
        ],
        "settings": {
          "foreground": "#F9C669"
        }
      },
      {
        "scope": "(text punctuation.definition.italic | text punctuation.definition.bold)",
        "settings": {
          "foreground": "#C594C5"
        }
      },
      {
        "scope": ["meta.diff", "meta.diff.header"],
        "settings": {
          "foreground": "#C594C5"
        }
      },
      {
        "scope": "markup.deleted",
        "settings": {
          "foreground": "#f08288"
        }
      },
      {
        "scope": "markup.inserted",
        "settings": {
          "foreground": "#99C794"
        }
      },
      {
        "scope": "markup.changed",
        "settings": {
          "foreground": "#EBCB8B"
        }
      },
      {
        "scope": "support.type.property-name",
        "settings": {
          "foreground": "#6796E6"
        }
      },
      {
        "scope": "constant.numeric.line-number.match",
        "settings": {
          "foreground": "#f08288"
        }
      },
      {
        "scope": "message.error",
        "settings": {
          "foreground": "#f08288"
        }
      },
      {
        "scope": "token.info-token",
        "settings": {
          "foreground": "#6796E6"
        }
      },
      {
        "scope": "token.warn-token",
        "settings": {
          "foreground": "#CD9731"
        }
      },
      {
        "scope": "token.error-token",
        "settings": {
          "foreground": "#F44747"
        }
      },
      {
        "scope": "token.debug-token",
        "settings": {
          "foreground": "#B267E6"
        }
      }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
      "class": {
        "foreground": "#5FB4B4"
      },
      "class.defaultLibrary": {
        "italic": true
      },
      "variable.defaultLibrary": {
        "italic": true
      },
      "parameter": {
        "foreground": "#F9C669",
        "italic": true
      },
      "parameter.declaration": {
        "italic": false
      },
      "variable": {
        "foreground": "#ddddd7"
      },
      "variable.readonly": {
        "foreground": "#d8b389"
      },
      "interface": {
        "foreground": "#5FB4B4",
        "italic": true
      },
      "interface.declaration": {
        "italic": false
      },
      "storage.type.property": {
        "fontStyle": "italic"
      },
      "property.declaration": {
        "foreground": "#86b9eb"
      },
      "property.readonly": {
        "foreground": "#f08288"
      },
      "method": {
        "foreground": "#86b9eb"
      },
      "enumMember": {
        "foreground": "#ddddd7",
        "italic": true
      }
    }
  }
  